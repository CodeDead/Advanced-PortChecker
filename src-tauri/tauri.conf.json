{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "build": {
    "beforeBuildCommand": "yarn build",
    "beforeDevCommand": "yarn dev",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5173"
  },
  "bundle": {
    "active": true,
    "category": "Utility",
    "copyright": "Copyright Â© 2025 CodeDead",
    "targets": "all",
    "externalBin": [],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": ""
    },
    "longDescription": "Advanced PortChecker is a TCP/IP port scanner that allows you to check if a port is open or not",
    "macOS": {
      "entitlements": null,
      "exceptionDomain": "",
      "frameworks": [],
      "providerShortName": null,
      "signingIdentity": null
    },
    "resources": [],
    "shortDescription": "A TCP/IP Port scanner",
    "linux": {
      "deb": {
        "depends": []
      }
    }
  },
  "productName": "advanced-portchecker",
  "mainBinaryName": "advanced-portchecker",
  "version": "2.2.0",
  "identifier": "com.codedead.advancedportchecker",
  "plugins": {},
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "fullscreen": false,
        "height": 780,
        "resizable": true,
        "title": "Advanced PortChecker",
        "width": 1100
      }
    ],
    "security": {
      "capabilities": [
        "main-capabilities"
      ],
      "csp": null
    }
  }
}
